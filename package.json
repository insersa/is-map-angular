{
  "name": "is-map-angular",
  "version": "17.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve is-map-angular-lib-app",
    "build": "ng build is-map-angular-lib-app",
    "build_lib": "ng-packagr -p projects/is-map-angular/ng-package.json",
    "npm_pack": "cd dist/is-map-angular && npm pack",
    "package": "npm run build_lib && npm run npm_pack",
    "test": "ng test",
    "test_jenkins": "ng test --browsers ChromeForJenkins --watch=false",
    "test_lib": "ng test is-map-angular",
    "test_lib_detail": "ng test is-map-angular --source-map=false",
    "test_lib_jenkins": "ng test is-map-angular --browsers ChromeForJenkins --watch=false",
    "lint": "npx eslint \"src/**/*.{js,ts,html}\" --max-warnings=0",
    "lint:fix": "eslint --fix \"src/**/*.{js,ts,html}\"",
    "lint_is_map_angular": "cd ./projects/is-map-angular && npx eslint \"src/**/*.{js,ts,html}\" --max-warnings=0",
    "lint_is_map_angular:fix": "cd ./projects/is-map-angular && npx eslint --fix \"src/**/*.{js,ts,html}\" --max-warnings=0",
    "e2e": "ng e2e",
    "doc": "compodoc -p tsconfig.json"
  },
  "lint-staged": {
    "src/**/*.{js,ts,html}": "npm run lint",
    "projects/is-map-angular/src/**/*.{js,ts,html}": "npm run lint_is_map_angular"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "17.3.1",
    "@angular/cdk": "17.3.1",
    "@angular/cli": "^17.3.0",
