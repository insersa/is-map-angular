    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-junit-reporter": "^1.2.0",
    "prettier": "^3.2.5",
    "protractor": "~7.0.0",
    "puppeteer": "^5.2.1",
